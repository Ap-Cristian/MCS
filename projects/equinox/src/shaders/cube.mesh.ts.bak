export class CubeVertex{

    private readonly cubeVertexArray = new Float32Array([
        -0.8, -0.8, // Triangle 1 (Blue)
        0.8, -0.8,
        0.8, 0.8,
        
        -0.8, -0.8, // Triangle 2 (Red)
        0.8, 0.8,
        -0.8, 0.8,
    ])

    public getCubeVertexArray(): Float32Array{
        return this.cubeVertexArray;
    }

};

export var cubeVertex = new CubeVertex;

